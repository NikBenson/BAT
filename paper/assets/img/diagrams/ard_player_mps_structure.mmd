    ImageTemplateWidthConfig "1" o-- "1" JSONDouble: min
    ImageTemplateWidthConfig "1" o-- "1" JSONDouble: max
    ImageTemplateWidthConfig "1" o-- "1" JSONDouble: stepSize
    ImageTemplateNamedSizeConfig "1" o-- "1" JSONDouble: minWidth
