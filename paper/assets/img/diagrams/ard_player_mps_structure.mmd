    WebConfig "1" o-- "0..1" JSONBoolean: disableBackButtonTitle
    WebConfig "1" o-- "0..1" JSONBoolean: isAudioSticky
    WebConfig "1" o-- "0..1" JSONBoolean: isForcedVideoView
