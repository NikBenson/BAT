    GenericConfig "1" o-- "0..1" JSONBoolean: isAutoplay
    GenericConfig "1" o-- "0..1" JSONBoolean: isMuted
    GenericConfig "1" o-- "0..1" JSONDouble: vodStartSeconds
    GenericConfig "1" o-- "0..1" JSONDouble: vodEndSeconds
    GenericConfig "1" o-- "0..1" JSONDouble: vodInitialPlayheadSeconds
    GenericConfig "1" o-- "0..1" JSONBoolean: isDvrEnabled
    GenericConfig "1" o-- "0..1" JSONDouble: volumePercent
    GenericConfig "1" o-- "0..1" ImageTemplateConfig: imageTemplateConfig
    ImageTemplateConfig "1" o-- "1" ImageTemplateWidthConfig: width
